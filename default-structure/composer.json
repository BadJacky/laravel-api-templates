{
  "name": "ibrunotome/laravel-api-templates",
  "type": "project",
  "description": "A Laravel API starter kit collection using different structures.",
  "keywords": [
    "framework",
    "laravel",
    "laravel-api",
    "laravel-skeleton",
    "laravel-ddd",
    "laravel-swoole",
    "swoole"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-bcmath": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "fideloper/proxy": "^4.1",
    "fntneves/laravel-transactional-events": "^2.0",
    "jenssegers/agent": "^2.6",
    "laravel/fortify": "^1.7",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/sanctum": "^2.9",
    "laravel/tinker": "^2.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "owen-it/laravel-auditing": "^12.0",
    "spatie/laravel-permission": "^3.0",
    "spatie/laravel-query-builder": "^3.0",
    "torann/geoip": "^3.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.7",
    "facade/ignition": "^2.3.6",
    "fakerphp/faker": "^1.13.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "nunomaduro/phpinsights": "dev-master",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "test": "phpunit --no-coverage",
    "phpcpd": "php phpcpd.phar app/ --fuzzy --progress",
    "static:analysis": "php artisan code:analyse --level=5",
    "insights": "php artisan insights --no-interaction --min-quality=90 --min-complexity=85 --min-architecture=90 --min-style=90",
    "lint": "php phpcs.phar",
    "lint:fix": "php phpcbf.phar"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
